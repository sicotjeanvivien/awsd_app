<VirtualHost *:80>
    ServerName awsd.fr
    ServerAlias www.awsd.fr

    DocumentRoot /awsd.fr/public
    <Directory /awsd.fr/public>
        AllowOverride All
        Order Allow,Deny
        Allow from All
    </Directory>

    # uncomment the following lines if you install assets as symlinks
    # or run into problems when compiling LESS/Sass/CoffeeScript assets
    # <Directory /awsd.fr>
    #     Options FollowSymlinks
    # </Directory>

</VirtualHost>